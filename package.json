{
	"name": "svelte-slate",
	"description": "slate svelte view layer",
	"version": "0.3.0",
	"license": "(MIT OR Apache-2.0)",
	"type": "module",
	"author": "Nathan Faucett",
	"repository": {
		"type": "git",
		"url": "https://github.com/nathanfaucett/svelte-slate"
	},
	"homepage": "https://github.com/nathanfaucett/svelte-slate#readme",
	"keywords": [
		"svelte",
		"slate",
		"editor",
		"text",
		"rich"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"module": "./dist/index.js",
	"main": "./dist/index.js",
	"files": [
		"dist"
	],
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js",
			"default": "./dist/index.js"
		}
	},
	"scripts": {
		"svelte-kit": "svelte-kit",
		"vite": "vite",
		"start": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"clean": "rm -rf node_modules/.vite .svelte-kit package build",
		"prepublishOnly": "svelte-package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "3",
		"@sveltejs/kit": "^2.7.0",
		"@sveltejs/package": "2",
		"@sveltejs/vite-plugin-svelte": "4",
		"@types/node": "20",
		"@types/prismjs": "1",
		"@typescript-eslint/eslint-plugin": "7",
		"@typescript-eslint/parser": "7",
		"eslint": "8",
		"eslint-config-prettier": "9",
		"lorem-ipsum": "2",
		"prettier": "3",
		"prettier-plugin-svelte": "3",
		"slate": "0",
		"slate-history": "0",
		"svelte": "5",
		"svelte-check": "3",
		"svelte-icons": "2",
		"svelte-portal": "2",
		"svelte-preprocess": "5",
		"svelte2tsx": "0",
		"tslib": "2",
		"typescript": "5",
		"vite": "5"
	},
	"peerDependencies": {
		"slate": "0",
		"svelte": ">=5",
		"tslib": "2"
	},
	"dependencies": {
		"@aicacia/debounce": "0",
		"fast-equals": "5",
		"scroll-into-view-if-needed": "3"
	},
	"packageManager": "pnpm@9.12.3+sha512.cce0f9de9c5a7c95bef944169cc5dfe8741abfb145078c0d508b868056848a87c81e626246cb60967cbd7fd29a6c062ef73ff840d96b3c86c40ac92cf4a813ee"
}